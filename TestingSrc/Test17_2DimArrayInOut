main {
    integer i, j, k, n, a, b;
    integer G[5,5];
    for (i := 0; i != 5; i++) {
        for (j := 0; j != 5; j++) {
            G[i,j] := 999;
            if (i == j) {
                G[i, j] := 0;
            }
        } 
    }
    read(n);
    while(n--) {
        read(a);
        read(b);
        read(G[a,b]);//still bug in read(w), G[a,b] := w;
    }
    for (i := 0; i != 5; i++) {
        for (j := 0; j != 5; j++) {
            write(G[i,j]);
        } 
        write("=================");
    }
}