main {
	integer a, b;
	read(a);
	while (a != 0) {
		a := a - 1;
		read(b);
		if (b == -1) {
			break;
		}
		else {
			while (b != 0) {
				b := b - 1;
				integer c;
				read(c);
				if (c == -2) {
					break;
				}
				else {
					write(b + c);
				}
			}
		}
	}
	/* jpc/jmp bug in line 7 15 may be in if-statement, fix it */
}